{
    "model": 
    {
        "version": "1.0",
        "model_name": "yolov3",
        "encrypt": false,
        "num_thread" : 4,
        "device" : "CPU",
        "model_file": "/path/to/model/file",
        "weight_file": "/path/to/weight/file",
        "backend": "ONNX",
        "dynamic_batch": false
    },
    "preprocess": 
    {
        "shape":[416,416],
        "mean": [0, 0, 0],
        "std": [1, 1, 1],
        "resize": "letterbox",
        "letterbox_color": [114, 114, 114],
        "channel_first": true,
        "format": "RGB",
        "dtype": "FLOAT32",
        "normalize": false,
        "scale": 1.0,
        "swapRB": false,
        "crop": false,
        "resize_mode": "LINEAR"
    },

    "infer": 
    {
        "input_names": ["input.1", "input.2"],
        "input_dims": [[1,3,320,320],[1,3,320,320]],
        "output_names": ["output.1"]
    },

    "postprocess": 
    {
        "type": "YOLO",
        "anchors": [10,13, 16,30, 33,23, 30,61, 62,45, 59,119, 116,90, 156,198, 373,326],
        "mask": [0,1,2],
        "num_classes": 80,
        "input_shape": [416,416],
        "anchors_scale": 32,
        "conf_thres": 0.5,
        "iou_thres": 0.4
    }
}