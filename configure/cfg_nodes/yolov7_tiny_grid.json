{
    "comment":
    {
        "device":["CPU","GPU"],
        "backend":["ONNX"],
        "resize_type":["Stretch", "LetterBox", "Fill"],
        "interp_mode":["Linear","Nearest","Cubic"],
        "channel_format":["RGB","BGR"],
        "dtype":["float32","uint8"],
        "input_format":["NCHW","NHWC"]
    },
    "model": 
    {
        "version": "1.0",
        "model_name": "yolov7_tiny_grid",
        "encrypt": false,
        "max_batch_size" : 4,
        "device" : "CPU",
        "model_file": "../models/yolov7/yolov7-tiny-grid.onnx",
        "weight_file": "../models/yolov7/yolov7-tiny-grid.onnx",        
        "backend": "ONNX",
        "dynamic_batch": false
    },
    "preprocess": 
    {
        "resize_type": "Stretch",
        "resize_channel": 3,
        "resize_width": 640,
        "resize_height": 640,
        "interp_mode": "Linear",
        "letterbox_color": [114, 114, 114],
        "channel_format": "RGB",

        "normalize": true,
        "mean": [0, 0, 0],
        "std": [255.0, 255.0, 255.0],

        "dtype": "float32",
        "input_format":"NCHW"

    },

    "infer": 
    {
        "input_names": ["images"],
        "input_dims": [[1,3,640,640]],
        "output_names": ["output"]
    },

    "postprocess": 
    {
        "post_process_type": "YoloGrid",
        "output_data_type": "DetectResult",
        "num_classes": 80,
        "confidence_threshold": 0.5,
        "nms_threshold":0.4
    }
}