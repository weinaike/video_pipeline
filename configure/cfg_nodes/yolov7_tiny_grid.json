{
    "model": 
    {
        "version": "1.0",
        "model_name": "yolov7_tiny_grid",
        "encrypt": false,
        "num_thread" : 4,
        "device" : "CPU",
        "model_file": "../models/yolov7/yolov7-tiny-grid.onnx",
        "weight_file": "../models/yolov7/yolov7-tiny-grid.onnx",        
        "backend": "ONNX",
        "dynamic_batch": false
    },
    "preprocess": 
    {
        "shape":[640,640],
        "mean": [0, 0, 0],
        "std": [255.0, 255.0, 255.0],
        "resize": "letterbox",
        "letterbox_color": [114, 114, 114],
        "channel_first": true,
        "format": "RGB",
        "dtype": "FLOAT32",
        "inputformat":"NCHW",
        "normalize": false,
        "scale": 1.0,
        "swapRB": false,
        "crop": false,
        "resize_mode": "LINEAR"
    },

    "infer": 
    {
        "input_names": ["images"],
        "input_dims": [[1,3,640,640]],
        "output_names": ["output"]
    },

    "postprocess": 
    {
        "type": "YOLO",
        "anchors": [10,13, 16,30, 33,23, 30,61, 62,45, 59,119, 116,90, 156,198, 373,326],
        "mask": [0,1,2],
        "num_classes": 80,
        "input_shape": [416,416],
        "anchors_scale": 32,
        "conf_thres": 0.5,
        "iou_thres": 0.4
    }
}